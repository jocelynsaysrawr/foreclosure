{
  "rules": {
    "no-spaced-func": 2,
    "space-in-parens": [
      2,
      "never",
      {
        "exceptions": []
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "yoda": [
      2,
      "never"
    ],
    "no-with": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "no-multiple-empty-lines": 2,
    "one-var": [
      2,
      "never"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": true
      }
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "wrap-iife": 2,
    "comma-style": [
      2,
      "last"
    ],
    "space-infix-ops": 2,
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "dot-notation": 2,
    "array-bracket-spacing": [
      2,
      "always"
    ],
    "curly": [
      2,
      "all"
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "lines-around-comment": [
      2,
      {
        "beforeLineComment": true
      }
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "quotes": [
      2,
      "single"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
